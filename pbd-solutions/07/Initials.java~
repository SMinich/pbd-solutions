class Initials
{
    public static void main( String[] args )
    {
	System.out.println("SSSSSSSSS     M       M");
	System.out.println("S             MM     MM");
	System.out.println("SSSSSSSSS     M M   M M");
	System.out.println("        S     M  M M  M");
	System.out.println("SSSSSSSSS     M   M   M");

    }
}
